<h1>Users#index</h1>

<% @users.each do |user| %>
<ul>
  
  <li> <%= user.first_name %>  <%= user.last_name %></li>
  <li><%= user.organization %></li>
  <li><%= user.biography %></li>
  <li><%= user.job_title %></li>
  <li><%= user.email %></li>
  <li><%= link_to 'Edit', edit_user_path(user) %></li>
</ul>
<% end %>